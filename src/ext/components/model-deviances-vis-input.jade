.flow-widget
  +subtitle('Model Deviances Visualization', 'link')

  table.flow-form
    tbody
      tr
        th(width='150')
          label Validation Frame:
        td
          select(data-bind="options:frames, value:selectedFrame, optionsCaption:'(Select)'")

  // ko if:selectedFrame
  +subtitle('Models', 'cubes')
  table.flow-form
    thead
      tr
        th(width='1' style='font-size:inherit')
          input(type='checkbox' title='Select or deselect all' data-bind="checked:checkAllModels")
        th(width='1') &nbsp;
        th Key
        th Algorithm
        th.flow-right Actions
    tbody
      // ko foreach:modelViews
      tr
        td
          input(type='checkbox' data-bind="checked:isChecked")
        td
          i.fa.fa-cube
        td
          +link('@key', 'view')
        td(data-bind='text:algo')
        td.flow-right 
          +button('Predict&hellip;', 'bolt', 'predict')
          //- +button('Clone&hellip;', 'copy', 'clone')
          +button('Inspect', 'list', 'inspect')
      // /ko
  // /ko

  div.flow-panel
    button.flow-button(type='button' data-bind='click:compute, enable:canCompute')
      i.fa.fa-link
      span Compute



